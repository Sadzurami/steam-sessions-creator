{
  "name": "steam-sessions-creator",
  "version": "2.1.0",
  "description": "App for creating Steam session files for Steam accounts",
  "author": "Sadzurami (https://github.com/Sadzurami)",
  "license": "MIT",
  "bin": {
    "ssc": "dist/main.js"
  },
  "pkg": {
    "assets": [
      "package.json",
      "node_modules/@doctormckay/steam-crypto",
      "node_modules/pino",
      "node_modules/pino-http",
      "node_modules/pino-pretty",
      "node_modules/thread-stream"
    ]
  },
  "scripts": {
    "build": "nest build",
    "build:package": "yarn run build && yarn pkg dist/main.js --target node18 --output ssc --config package.json --compress GZip",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "chalk": "^4",
    "log-update": "^4.0.0",
    "nest-commander": "^3.12.0",
    "nestjs-pino": "^3.5.0",
    "p-queue": "6.6.2",
    "pino": "^8.16.1",
    "pino-http": "^8.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "steam-session": "^1.7.1",
    "steam-totp": "^2.1.2",
    "steamid": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.78",
    "@types/node": "20.8.9",
    "@types/steam-totp": "^2.1.1",
    "@types/steamid": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "pkg": "^5.8.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  }
}
